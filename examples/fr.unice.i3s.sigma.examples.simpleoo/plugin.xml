<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin
      name="%pluginName"
      id="fr.unice.i3s.sigma.examples.simpleoo"
      version="1.0.0"
      provider-name="%providerName">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.emf.ecore" export="true"/>
   </requires>

   <runtime>
      <library name=".">
         <export name="*"/>
      </library>
   </runtime>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated SimpleOO -->
      <package
            uri="http://sigma.i3s.unice.fr/examples/simpleoo/1.0"
            class="fr.unice.i3s.sigma.examples.simpleoo.SimpleooPackage"
            genModel="model/SimpleOO.genmodel"/>
   </extension>

</plugin>
