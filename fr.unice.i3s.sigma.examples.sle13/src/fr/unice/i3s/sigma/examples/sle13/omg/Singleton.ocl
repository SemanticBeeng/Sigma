import uml : 'http://uml/1.0'

package uml

context Class

inv HasGetInstance: 
  self.getGetInstanceOperation().oclIsDefined()

inv GetInstanceIsStatic: 
  HasGetInstance implies self.getGetInstanceOperation().ownerScope = ScopeKind.sk_classifier

--def: getGetInstanceOperation : Operation = self.operations->select(o|o.name = "getInstance")->first()

endpackage